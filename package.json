    "gulp-coffee": "2.3.1",
    "gulp-jshint": "1.12.0",
    "gulp-load-plugins": "0.10.0",
    "gulp-nodemon": "2.0.4",
    "gulp-sass": "1.3.3",
    "gulp-size": "1.3.0",
    "gulp-sourcemaps": "1.6.0",
    "gulp-strip-debug": "1.0.2",
    "gulp-uglify": "1.4.2",
    "gulp-useref": "1.3.0",
    "gulp-util": "3.0.7",
    "gulp-webserver": "0.9.1",
    "jest-cli": "0.4.19",
    "main-bower-files": "2.9.0",
