<div class="row">
  <div class="page-header">
    <h2>Interleague Game Reporting Form Importer</h2>
  </div>

  <div class="well well-sm">
    <%= bootstrap_form_for @form, url: interleague_game_reporting_forms_path, layout: :horizontal do |f| %>
      <div id="fileinput-error" class="alert alert-danger"></div>

      <%= f.file_field :form %>

      <div class="form-group">
        <div class="col-sm-offset-2 col-sm-10">
          <button type="submit" class="btn btn-default">Import</button>
        </div>
      </div>
    <% end %>
  </div>
</div>
